<view><view class="{{['grace-article-title',graceSkeleton=='ing'?'grace-skeleton':'']}}">{{article.artTitle}}</view><view class="grace-article-author-line"><view class="{{['grace-article-author',graceSkeleton=='ing'?'grace-skeleton':'']}}"><image src="{{article.uface}}" mode="widthFix"></image><view class="author-name">{{article.uname}}</view></view><view>{{article.artCreatetime}}</view></view><view class="grace-article-contents"><block wx:for="{{artContents}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{item.type=='image'}}"><view class="{{['img-item',graceSkeleton=='ing'?'grace-skeleton':'']}}"><image src="{{item.content}}" data-url="{{item.content}}" mode="widthFix" data-event-opts="{{[['tap',[['showImgs',['$event']]]]]}}" bindtap="__e"></image></view></block><block wx:if="{{item.type=='text'}}"><view class="{{['text-item',graceSkeleton=='ing'?'grace-skeleton':'']}}">{{item.content}}</view></block></block></block></view></view>